citHeader("To cite variantMapper in publications use:")

citEntry(
  entry    = "Unpublished",
  title    = "variantMapper: An R package that assigns missense and SNVs an EVE score (prediction of pathogenicity)",
  author   = "Angela Ng",
  note     = "Unpublished",
  year     = "2022",

  textVersion =
  paste("Ng, A. (2022) variantMapper: An R package that assigns missense and SNVs an EVE score (prediction of
  pathogenicity). URL https://github.com/theangelang/variantMapper."

  )
)
